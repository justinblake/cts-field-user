page-single-foreman-task {

  .card-md {
    border: 4px solid #EFEFEF;
  }

  .card-ios {
    border: 4px solid #EFEFEF;
  }

  .task-card-header {
    font-size: 2.3rem;
  }

  .job-status {
    margin: 10px 18px 10px 16px;
    font-size: 1.6rem;
    background-color: white;
    border: 2px solid white;
    border-radius: 5px;
    padding: 4px 4px 4px 10px;
  }

  .emp-font {
    font-size: 1.7rem;
    padding-left: 3px;
    font-weight: bold;
  }

  .card-width {
    margin-left: -3px;
  }

  .proj-header {
    font-size: 2.2rem;
    padding-bottom: 0;
  }

  .margin-left18 {
    margin-left: 18px;
  }

  ul {
    padding-left: 35px;
  }

  .indent19 {
    margin-left: 22px !important;
  }

  .foremanPhone {
    font-size: 1.6rem !important;
    margin-left: 4px;
  }

  .card-ios + ion-card {
    margin-top: -15px;
  }

  .ios-margin {
    margin-bottom: -13px;
  }

  .sent {
    background-color: rgba(251, 245, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(251, 245, 0);
  }
  .accepted {
    background-color: rgba(251, 245, 0, .4);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(251, 245, 0);
  }
  .started {
    background-color: rgba(0, 205, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(0, 205, 0);
  }
  .cancelled {
    background-color: rgba(128, 127, 131, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(128, 127, 131);
  }
  .completed {
    background-color: rgba(0, 128, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(0, 128, 0);
  }
  .emergency {
    background-color: rgba(191, 0, 0, .6);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(191, 0, 0);
  }
  .rejected {
    background-color: rgba(230, 0, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(230, 0, 0);
  }
  .temporary {
    background-color: rgba(254, 140, 4, .4);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(254, 140, 4);
  }

  h4 > span + span {
    font-size: 15px;
  }

  .mar-bot10 {
    margin-bottom: 10px;
  }

  .link-blue {
    color: #3F51B5;
    font-weight: 700;
  }

  .equip-name {
    font-size: 1.7rem;
    font-weight: 500;
    color: #3F51B5;
  }

  .equip-assig {
    font-size: 1.6rem;
    margin-top: 4px;
  }

  .equip-bold {
    font-weight: bold;
  }

  .equip-div {
    margin: 10px 0;
  }

  .list-ios > .item-block:first-child {
    border: none;
  }

  .card-ios {
    margin-bottom: 28px;
  }

  .card-ios p {
    color: black;
    letter-spacing: 0.5px;
  }

  .no-images {
    color: white;
    font-size: 1.6rem;
    margin: 10px 0px 10px 18px;
  }

  .contractor-phone {
    color: black;
    margin-right: 5px;
  }

  .call-contractor {
    font-size: 1.3rem !important;
    margin-left: -4px;
  }

  .contractor-title2 {
    margin: 6px 0px 8px 4px;
  }

}
