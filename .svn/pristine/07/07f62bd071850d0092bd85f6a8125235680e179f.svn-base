page-timecard {
  .list-md {
    margin-top: 1px;
  }

  .btn-margin {
    margin-left: 9px;
  }

  .first-list {
    margin-left: -11px;
  }

  .notes > .item-inner {
    border-bottom-color: #3F51B5;
  }

  .notes {
    padding-right: 8px;
  }

  .item-md.item-input.ng-valid.input-has-value:not(.input-has-focus) .item-inner {
    border-bottom-color: #3F51B5;
    -webkit-box-shadow: inset 0 -1px 0 0 #3F51B5;
    box-shadow: inset 0 -1px 0 0 #3F51B5;
  }

  .item-md.item-input.ng-untouched .item-inner {
    border-bottom-color: #999;
    -webkit-box-shadow: inset 0 -1px 0 0 #999;
    box-shadow: inset 0 -1px 0 0 #999;
  }

}
