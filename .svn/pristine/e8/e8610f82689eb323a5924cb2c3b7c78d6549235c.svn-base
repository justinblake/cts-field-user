page-home {

  ion-footer {
    background-color: #EFEFEF;
  }

  .card p {
    color: #000;
    font-size: 1.7rem;
  }
  .s-time {
    margin-top: 15px;
    font-weight: 600;
  }

  .gen-mar-bot20 {
    margin-bottom: 20px;
  }

  .fs-no-results-msg {
    position: relative;
    animation: mymove 2s;
    animation-iteration-count: 4;
    animation-timing-function: ease-in-out;
    color: red;
    animation-delay: 1s;
    animation-direction: alternate;
  }

  @keyframes mymove {
    from {
      top: 0;
    }
    to {
      top: -75px;
    }

  }

  .mar-top10 {
    margin-top: 10px;
  }

  .content-ios > .scroll-content {
    margin-top: 90px !important;
  }

  .card-header-md {
    font-size: 1.8rem;
  }

  .bottom-pad {
    padding-bottom: 11px;
  }

  .pad-bot18 {
    padding-bottom: 18px;
  }

  .android-title > .toolbar-title-md {
    font-size: 1.4rem;
  }

  .comp-wrap {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    max-width: 175px;
    padding-left: 10px;
  }

  .button-ios {
    height: 2.4em;
  }

  .grid {
    padding: 1px 5px 5px 5px;
  }

  .button-md {
    height: 4rem;
    padding-top: 2px;
  }

  @media (min-width: 685px) {
    .comp-wrap {
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis;
      max-width: 500px;
      padding-left: 10px;
    }
  }

  .mar-bot60 {
    margin-bottom: 60px;
  }

  .mar-bot25 {
    margin-bottom: 25px;
  }

  .pad-bot30 {
    padding-bottom: 30px;
  }

  .bold {
    font-weight: bold;
  }



  .task-button {
    border-bottom: 2px solid #3F51B5;
    padding-left: 0;
  }


  ion-grid ion-row ion-col {
    height: 40px;
  }

  ion-footer ion-grid {
    height: 62px;
  }

  .laborer-btn {
    color: white;
    font-weight: bolder;
    background-color: #57AD57;
    border-radius: 7px;
    box-shadow: 4px 4px 4px 0 rgba(0, 0, 0, 0.85);
    width: 100%;
    font-size: 1.7rem;
  }

  .newTime {
    top: 14px;
    color: #3F51B5;
    font-size: 1.6rem;
    font-weight: 500;
  }

  .task-icon {
    top: 16px;
    right: -30px;
    color: #3F51B5;
  }

  .task-text {
    top: 11px;
    color: #3F51B5;
    font-size: 1.8rem;
    font-weight: 500;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    max-width: 200px;
  }

  @media (max-width: 376px) {
    .task-icon {
      right: -10px;
    }
    .laborer-btn {
      width: 120%;
      font-size: 1.6rem;
      margin-left: -10px;
    }
  }

  @media (max-width: 350px) {
    .task-icon {
      right: -5px;
    }
    .newTime {
      color: white;
      top: 0;
    }
    .laborer-btn {
      width: 130%;
      font-size: 1.5rem;
      margin-left: -15px;
    }
  }

}

.list-font {
  font-size: 1.5rem;
}

.multiple-suppliers {
  margin-bottom: -35px;
}
