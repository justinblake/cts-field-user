page-foreman {
  .dir-coverage {
    background-color: #efefef;
    margin-left: -16px;
    width: 113%;
  }

  .card-md {
    border: 4px solid #EFEFEF;
  }

  .card-ios {
    border: 4px solid #EFEFEF;
  }

  .task-card-header {
    font-size: 1.8rem;
  }

  .card-margin {
    margin-left: -31px;
    margin-right: -31px;
  }

  .card-margin > .card-md {
    box-shadow: none;
    border-radius: 0;
    border-bottom: none;
  }

  .job-status {
    margin: 10px 18px 10px 18px;
    font-size: 1.7rem;
    background-color: white;
    border: 2px solid white;
    border-radius: 5px;
    padding: 4px 4px 4px 10px;
  }

  .emp-font {
    font-size: 1.7rem;
    padding-left: 3px;
  }

  .start-time {
    font-size: 2rem;
    margin-left: 30px;
  }

  .button-margin {
    margin-top: 15px;
  }

  .more-margin {
    margin-left: -4px;
  }

  .card-width {
    margin-left: -3px;
  }

  .proj-header {
    font-size: 2.2rem;
  }

  .margin-left18 {
    margin-left: 18px;
  }

  .no-bullet {
    list-style-type: none;
    font-size: 1.3rem;
  }

  .li-notes {
    font-size: 1.7rem;
  }

  ul {
    padding-left: 35px;
  }

  .indent19 {
    margin-left: 22px !important;
  }

  .foremanPhone {
    font-size: 1.6rem !important;
    margin-left: 0px;
  }

  .margin-10 {
    margin-left: -9px;
  }

  .card-ios + ion-card {
    margin-top: -15px;
  }

  .ios-margin {
    margin-bottom: -13px;
  }

  .gen-mar-bot20 {
    margin-bottom: 20px !important;
  }
  .sent {
    background-color: rgba(251, 245, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(251, 245, 0);
  }
  .accepted {
    background-color: rgba(251, 245, 0, .4);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(251, 245, 0);
  }
  .started {
    background-color: rgba(0, 205, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(0, 205, 0);
  }
  .completed {
    background-color: rgba(0, 128, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(0, 128, 0);
  }
  .emergency {
    background-color: rgba(191, 0, 0, .6);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(191, 0, 0);
  }
  .rejected {
    background-color: rgba(230, 0, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(230, 0, 0);
  }
  .temporary {
    background-color: rgba(254, 140, 4, .4);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(254, 140, 4);
  }

  h4 > span + span {
    font-size: 15px;
  }

  .mar-bot10 {
    margin-bottom: 10px;
  }

  .link-blue {
    color: #3F51B5;
    font-weight: 700;
  }



}
