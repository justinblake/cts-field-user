.content-ios, .content-md {
  background-color: #A2A2A2;
}

.card-ios, .card-md {
  background-color: #FFF;
}

.card-ios {
  border-radius: 7px;
  box-shadow: 5px 5px 5px 0 rgba(0, 0, 0, 0.25);
}

.card-md {
  margin: 15px 10px 10px 10px;
  width: calc(100% - 20px);
  border-radius: 5px;
  font-size: 1.4rem;
  background: #fff;
  box-shadow: 5px 5px 5px 0 rgba(0, 0, 0, 0.25);
}

.button-ios {
  border-radius: 7px;
}

.card-md + ion-card {
  margin-top: 15px;
}

.card-header-ios, .card-header-md {
  color: orange;
  font-weight: bold;
}

.task-card-header {
  background-color: #EFEFEF;
}

.task-card-content {
  padding-top: 10px;
  .list-ios > .item-block:last-child, .list-ios > .item-wrapper:last-child .item-block {
    border: 0px solid transparent;
  }
  .item, .item-ios, .list-ios {
    background-color: transparent;
    border: 0px solid transparent;
  }
  ol {
    padding-left: 20px;
  }
  .ion-ios-map::before {
    color: #337AB7;
  }
}

h3.text-info {
  color: #3F51B5;
  font-weight: 500;
  font-size: 2.2rem;
}

p.equip-name {
  color: #3F51B5 !important;
  font-weight: 500;
  font-size: 2.2rem;
}

h4.text-info {
  color: #3F51B5;
  font-weight: 500;
  font-size: 2.0rem !important;
}

.capitalize {
  text-transform: capitalize;
}

ion-card.driving-directions {
  .address {
    background-color: #efefef;
  }
}

[cts-header] {
  font-weight: bold;
}

.cts-card.info {
  color: darken(color($colors, info), 35%);
  background-color: lighten(color($colors, info), 35%);
  border-top: 5px solid darken(color($colors, info), 20%);
  .action-row {
    display: none;
  }
}

.cts-card.success {
  color: darken(color($colors, secondary), 25%);
  background-color: lighten(color($colors, secondary), 35%);
  border-top: 5px solid color($colors, secondary);
}

.cts-card.warning {
  color: darken(color($colors, warning), 25%);
  background-color: lighten(color($colors, warning), 30%);
  border-top: 5px solid color($colors, warning);
  .action-row {
    display: none;
  }
}

.cts-card.danger {
  color: darken(color($colors, danger), 25%);
  background-color: lighten(color($colors, danger), 35%);
  border-top: 5px solid color($colors, danger);
  .action-row {
    display: none;
  }
}

.fs-no-results-container {
  width: 100vw;
  background-color: #efefef;
  text-align: center;
  position: absolute;
  top: 163px;
  right: 0;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;

  .content {
    .fs-no-results-msg {
      letter-spacing: .1em;
      text-transform: uppercase;
