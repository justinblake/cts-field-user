page-foreman {
  .dir-coverage {
    background-color: #efefef;
    margin-left: -16px;
    width: 113%;
  }

  .card-md {
    border: 4px solid #EFEFEF;
  }

  .card-ios {
    border: 4px solid #EFEFEF;
  }

  .task-card-header {
    font-size: 2.3rem;
  }

  .card-margin {
    margin-left: -31px;
    margin-right: -31px;
  }

  .card-margin > .card-md {
    box-shadow: none;
    border-radius: 0;
    border-bottom: none;
  }

  .job-status {
    margin: 10px 18px 10px 16px;
    font-size: 1.6rem;
    background-color: white;
    border: 2px solid white;
    border-radius: 5px;
    padding: 4px 4px 4px 10px;
  }

  .emp-font {
    font-size: 1.7rem;
    padding-left: 3px;
  }

  .start-time {
    font-size: 2rem;
    margin-left: 30px;
  }

  .button-margin {
    margin-top: 15px;
  }

  .more-margin {
    margin-left: -4px;
  }

  .card-width {
    margin-left: -3px;
  }

  .proj-header {
    font-size: 2.2rem;
    padding-bottom: 0;
  }

  .contractorPhoneNum {
    margin-left: 0;
    font-size: 1.6rem;
    margin-top: -7px;
    padding-left: 0;
  }

  .cont-p {
    font-size: 1.6rem;
    font-weight: 600;
    letter-spacing: 1px;
  }

  .cont-wrap {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    max-width: 194px;
  }

  .margin-left18 {
    margin-left: 18px;
  }

  .no-bullet {
    list-style-type: none;
    font-size: 1.3rem;
  }

  .li-notes {
    font-size: 1.7rem;
  }

  ul {
    padding-left: 35px;
  }

  .indent19 {
    margin-left: 22px !important;
  }

  .foremanPhone {
    font-size: 1.6rem !important;
    margin-left: 4px;
  }

  .margin-10 {
    margin-left: -9px;
  }

  .card-ios + ion-card {
    margin-top: -15px;
  }

  .ios-margin {
    margin-bottom: -13px;
  }

  .gen-mar-bot20 {
    margin-bottom: 20px !important;
  }
  .sent {
    background-color: rgba(251, 245, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(251, 245, 0);
  }
  .accepted {
    background-color: rgba(251, 245, 0, .4);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(251, 245, 0);
  }
  .started {
    background-color: rgba(0, 205, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(0, 205, 0);
  }
  .cancelled {
    background-color: rgba(128, 127, 131, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(128, 127, 131);
  }
  .completed {
    background-color: rgba(0, 128, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(0, 128, 0);
  }
  .emergency {
    background-color: rgba(191, 0, 0, .6);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(191, 0, 0);
  }
  .rejected {
    background-color: rgba(230, 0, 0, .3);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(230, 0, 0);
  }
  .temporary {
    background-color: rgba(254, 140, 4, .4);
    margin-top: 5px;
    border-radius: 5px;
    border: 2px solid rgb(254, 140, 4);
  }

  h4 > span + span {
    font-size: 15px;
  }

  .mar-bot10 {
    margin-bottom: 10px;
  }

  .link-blue {
    color: #3F51B5;
    font-weight: 700;
  }

  @media (max-width: 375px) {
    .cont-wrap {
      max-width: 141px;
    }

    .contractorPhoneNum {
      font-size: 1.5rem;
    }

  }

  @media (max-width: 321px) {
    .cont-wrap {
      max-width: 141px;
    }

    .contractorPhoneNum {
      margin-left: -3px;
      font-size: 1.5rem;
      margin-top: -7px;
    }

    page-foreman .cont-p {
      font-size: 1.3rem;
    }

  }

  @media (max-width: 328px) {
    .newTime {
      font-size: 1.5rem;
    }
  }

  .address {
    padding-top: 0;
  }

  ion-col[width-40] {
    padding-bottom: 0;
  }

  ion-col[width-60] {
    padding-bottom: 0;
  }

  .dir-pad {
    padding-left: 10px;
  }

  .hideDetails {
    position: relative;
    right: -46%;
  }

  .next-space > .card-border > .task-card-header {
    padding: 5px 10px 5px 16px;
  }

  .next-space > .card-border {
    margin-left: 0;
    margin-right: 0;
    width: 100%;
  }

  .card-space {
    padding-bottom: 25px;
    margin-bottom: 25px;
  }

  .card-space > .task-card-header {
    font-size: 2.5rem;
  }

  .move-up {
    margin-top: -30px;
  }

  .mar-bot25 {
    margin-bottom: 25px;
  }

  .more-left {
    padding-left: 9px;
  }

  .pad8 {
    padding-right: 8px;
  }

  .equip-name {
    font-size: 1.7rem;
    font-weight: 500;
    color: #3F51B5;
  }

  .equip-assig {
    font-size: 1.6rem;
    margin-top: 4px;
  }

  .equip-bold {
    font-weight: bold;
  }

  .equip-div {
    margin: 10px 0;
  }

  .list-ios>.item-block:first-child {
    border: none;
  }

  .card-ios {
    margin-bottom: 28px;
  }

  .card-ios p {
    color: black;
    letter-spacing: 0.5px;
  }

  .fs-no-results-container-ios {
    top: 58px;
  }

  .contractorPhone {
    margin-left: 12px;
  }

  .contractor-title {
    margin-left: 21px;
    margin-top: 7px;
  }

}
