<ion-header>

  <ion-navbar>
    <ion-title>Add Feedback</ion-title>
  </ion-navbar>

</ion-header>


<ion-content no-padding>
  <ion-card><ion-card-content>
  <ion-grid>
    <ion-row>

        <div><strong>Enter Feedback</strong></div>


    </ion-row>
      <ion-row>
          <ion-textarea class="" [(ngModel)]="data.notes" placeholder="" (blur)="trimNotes()"></ion-textarea>
      </ion-row>
  </ion-grid>
  </ion-card-content>


      <ion-row style="background-color:white">
        <ion-col width-50>
          <button ion-button icon-left block color="secondary" (click)="save()">
            <ion-icon name="checkbox-outline"></ion-icon>
            Save
          </button>
        </ion-col>
        <ion-col width-50>
          <button ion-button icon-left block color="dark" (click)="cancel()">
            <ion-icon name="close"></ion-icon>
            Cancel
          </button>
        </ion-col>
      </ion-row>


  </ion-card>
</ion-content>
