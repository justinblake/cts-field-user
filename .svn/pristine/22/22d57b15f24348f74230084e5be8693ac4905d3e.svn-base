page-home {

  ion-footer {
    background-color: #EFEFEF;
    box-shadow: rgba(0, 0, 0, 0.35) 0px -2px 3px 1px;
  }

  .card p {
    color: #000;
    font-size: 1.7rem;
  }

  .fs-no-results-msg {
    position: relative;
    animation: mymove 2s;
    animation-iteration-count: 4;
    animation-timing-function: ease-in-out;
    color: red;
    animation-delay: 1s;
    animation-direction: alternate;
  }

  @keyframes mymove {
    from {
      top: 0;
    }
    to {
      top: -75px;
    }
  }

  .mar-top10 {
    margin-top: 10px;
  }

  .mar-top13 {
    margin-top: 13px;
  }

  .content-ios > .scroll-content {
    margin-top: 74px !important;
  }

  .card-header-md {
    font-size: 2.5rem;
  }

  .card-header-ios {
    font-size: 2.5rem;
  }

  .list-ios > .item-block:first-child {
    border: none;
  }

  .bottom-pad {
    padding-bottom: 11px;
  }

  .pad-bot18 {
    padding-bottom: 18px;
  }

  .android-title > .toolbar-title-md {
    font-size: 1.4rem;
  }

  .comp-wrap {
    overflow: hidden;
    text-overflow: ellipsis;
    width: 169px;
    padding-left: 10px;
  }

  .button-ios {
    height: 2.4em;
  }

  .grid {
    padding: 1px 5px 5px 5px;
  }

  .button-md {
    height: 4rem;
    padding-top: 2px;
  }

  @media (min-width: 685px) {
    .comp-wrap {
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis;
      max-width: 169px;
      padding-left: 10px;
    }
  }

  @media (max-width: 350px) {
    .hide-task {
      display: none
    }
  }

  @media (min-width: 351px) {
    .show-task {
      display: none
    }
  }

  .mar-bot75 {
    margin-bottom: 75px;
  }

  .mar-bot25 {
    margin-bottom: 25px;
  }

  .pad-bot20 {
    padding-bottom: 20px;
  }

  .bold {
    font-weight: bold;
  }

  ion-grid ion-row ion-col {
    height: 40px;
  }

  ion-footer ion-grid {
    height: 62px;
  }

  .task-icon {
    top: 16px;
    right: -20px;
    color: #3F51B5;
  }

  @media (max-width: 376px) {
    .task-icon {
      right: -10px;
    }
  }

  @media (max-width: 350px) {
    .task-icon {
      right: -5px;
    }
    .newTime {
      color: white;
      top: -2px;
    }

    .timecard-hide {
      display: none;
    }

    .timecard-show {
      display: inherit;
    }
  }

  .manage-app.card-md {
    margin-top: 6px;
  }

  .manage-app.card-ios {
    margin-top: 13px;
  }

  .manage-app > .card-header-md {
    height: 80px;
  }

  .manage-app > .card-header-ios {
    height: 80px;
  }

  .pad-top4 {
    padding-top: 4px;
  }

  .clockOut {
    color: #d9534f;
  }

  .clockIn {
    color: #57AD57;
  }

  .orange {
    color: orange;
  }

  .scroll-content {
    margin-top: 70px !important;
  }

  .clock-out {
    margin-top: -7px;
  }

  .unassigned {
    color: black;
    font-size: 1.8rem;
  }

  .time-font {
    font-size: 2.2rem;
  }

  .accept-icon {
    top: 16px;
    right: 4px;
  }

  .contractConts {
    margin: 5px 0;
  }

  //.content-ios > .scroll-content > .timecard-margin {
  //  margin-top: 26px !important;
  //}

  @media (max-width: 426px) {
    .next-space {
      width: 104%;
      margin-left: -2%;
    }
  }

  @media (max-width: 376px) {

    .next-space {
      width: 105%;
      margin-left: -3%;
    }
  }

  @media (max-width: 350px) {

    .next-space {
      width: 107%;
      margin-left: -4%;
    }
  }

  .laborer-action {
    margin-left: 0;
    width: 100%;
  }


}



